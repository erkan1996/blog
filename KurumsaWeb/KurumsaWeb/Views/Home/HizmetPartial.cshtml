@model IEnumerable<KurumsaWeb.Models.Model.Hizmet>
@{ Layout = null;}
<div class="col-lg-12">
	<h4 class="heading">Hizmetlerimiz</h4>
	<div class="row">
		<section id="projects">
			<ul id="thumbs" class="portfolio">
				<!-- Item Project and Filter Name -->
				@foreach (var item in Model)
				{
					<li class="col-lg-3 design" data-id="id-0" data-type="web">
					<div class="item-thumbs">
						<!-- Fancybox - Gallery Enabled - Title - Full Image -->
						<a class="hover-wrap fancybox" data-fancybox-group="gallery" title="@item.Baslik  @item.Aciklama" href="">
							<img src="@item.ResimURL">
							<span class="overlay-img"></span>
							<span class="overlay-img-thumb font-icon-plus"></span>
						</a>
						<!-- Thumb Image and Description -->
						<img src="@item.ResimURL" alt="@item.Baslik">
					</div>
				</li>
				}
				
				<!-- End Item Project -->
			
			</ul>
		</section>
	</div>
</div>